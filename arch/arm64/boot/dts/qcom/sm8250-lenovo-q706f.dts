	status = "okay";
	firmware-name = "qcom/sm8250/q706f/cdsp.mbn";
};

&crypto {
	status = "disabled";
};

&gmu {
	status = "okay";
};

&gpi_dma0 {
	status = "okay";
};

&gpi_dma1 {
	status = "okay";
};

&gpi_dma2 {
	status = "okay";
};

&gpu {
	status = "okay";

